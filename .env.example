# Password for the 'elastic' user (at least 6 characters)
ELASTIC_PASSWORD=elastic123

# Password for the 'kibana_system' user (at least 6 characters)
KIBANA_PASSWORD=kibana123

# Version of Elastic products
STACK_VERSION=8.6.2

# Set the cluster name
CLUSTER_NAME=docker-cluster

# Set to 'basic' or 'trial' to automatically start the 30-day trial
LICENSE=basic

# Port to expose Elasticsearch HTTP API to the host
ES_PORT=9200

# Port to expose Kibana to the host
KIBANA_PORT=5601

# Increase or decrease based on the available host memory (in bytes)
MEM_LIMIT=1073741824

# APM Server Configuration
APM_SERVER_HOST=0.0.0.0
APM_SERVER_PORT=8200
APM_SERVER_SERVICE_NAME=apm-server

# Kibana Service Configuration
KIBANA_SERVICE_NAME=kibana

# APM Integration Configuration
APM_POLICY_NAME=APM Server policy
APM_POLICY_DESCRIPTION=Policy for APM Server
APM_NAMESPACE=default
APM_INTEGRATION_NAME=apm-integration
APM_INTEGRATION_DESCRIPTION=APM integration for server

# APM Setup Configuration
APM_SETUP_WAIT_INTERVAL=10
